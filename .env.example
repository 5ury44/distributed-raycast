# Raycast FPS Game Environment Configuration
# Copy this file to .env and update the values as needed

# Load Balancer Configuration
WORKER_ENDPOINT=your-load-balancer-endpoint:50051

# Server Configuration
WORKER_SERVER_ADDRESS=0.0.0.0:50051
WORKER_SERVICE_NAME=raycast-worker
WORKER_NAMESPACE=default

# Port Configuration
WORKER_PORT=50051
MASTER_PORT=50052

# Health Check Configuration
HEALTH_CHECK_INTERVAL_SECONDS=30
HEALTH_CHECK_TIMEOUT_SECONDS=5
REQUEST_TIMEOUT_SECONDS=30

# Worker Pool Configuration
DISCOVERY_INTERVAL_SECONDS=30
MAX_WORKER_CONNECTIONS=3

# Game Configuration
SCREEN_WIDTH=1024
SCREEN_HEIGHT=768
FOV=60.0
FPS_LIMIT=60

# Player Configuration
MOVE_SPEED=0.1
ROTATION_SPEED=0.05
